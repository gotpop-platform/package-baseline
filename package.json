    "test": "echo \"No tests specified\" && exit 0",
    "pub": "dotenv npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gotpop-platform/package-baseline.git"
  },
  "author": "GotPop",
  "license": "MIT",
  "keywords": [
    "utils",
    "npm",
    "package"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@types/bun": "^1.1.11",
    "@types/node": "^22.7.4",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "dotenv": "^16.4.5"
  },
  "dependencies": {
    "@gotpop-platform/package-logger": "workspace:*"
  },
  "bun": {
    "version": ">=1.1.37"
